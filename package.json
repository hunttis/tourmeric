{
  "name": "tourmeric",
  "version": "1.0.0",
  "description": "This project is for a tournament calendar that let's you create events, edit them and then allows users to pre-register for the events",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/hunttis/tourmeric.git"
  },
  "author": "Ville Nuutinen",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/hunttis/tourmeric/issues"
  },
  "browserslist": [
    "since 2017-06"
  ],
  "homepage": "https://gitlab.com/hunttis/tourmeric#README",
  "scripts": {
    "dev": "parcel ./src/index.html",
    "clean": "rm -rf .cache; rm -rf deploy; rm -rf testing",
    "copy-html": "cp deploy/index.html functions/index.html",
    "dev-testing": "DEPLOYMENT=TESTING NODE_ENV=development parcel ./src/index.html -d testing",
    "dev-production": "NODE_ENV=production parcel ./src/index-prod.html -o index.html ",
    "build": "PARCEL_WORKERS=1 parcel build ./src/index-prod.html -o index.html  -d deploy --public-url . --no-source-maps --detailed-report; yarn copy-html",
    "build-mapped": "parcel build ./src/index.html -d deploy --public-url .; yarn copy-html",
    "bolt": "firebase-bolt < firebase-rules/firebase-rules.bolt > firebase-rules/rules.json",
    "lint": "eslint . --ext tsx,ts,js,jsx",
    "test": "jest",
    "test-watch": "jest --watch",
    "all-tests": "yarn test; yarn lint; yarn check-translations",
    "check-translations": "json-diff --keys-only src/translations/en.json src/translations/fi.json",
    "firebase-serve": "firebase serve",
    "firebase-login": "firebase login",
    "firebase-init": "firebase init",
    "firebase-deploy": "yarn clean; yarn bolt; yarn build; firebase use default; firebase deploy",
    "firebase-quick-deploy": "yarn build; yarn bolt; firebase deploy",
    "firebase-deploy-testing": "yarn clean; yarn bolt; DEPLOYMENT=TESTING parcel build ./src/index.html -d deploy --public-url . --no-source-maps; yarn copy-html; firebase use testing; firebase deploy",
    "firebase-deploy-rules": "yarn bolt; firebase use default; firebase deploy --only database",
    "firebase-deploy-rules-testing": "yarn bolt; firebase use testing; firebase deploy --only database",
    "firebase-deploy-rules-localdev": "yarn bolt; firebase use localdev; firebase deploy --only database",
    "firebase-deploy-functions": "firebase use default; firebase deploy --only functions",
    "firebase-deploy-functions-testing": "firebase use testing; firebase deploy --only functions"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "2.3.3",
    "@typescript-eslint/parser": "2.3.3",
    "bulma": "^0.7.5",
    "bulma-extensions": "^1.0.30",
    "bulmaswatch": "^0.7.5",
    "classnames": "^2.2.6",
    "clipboard": "^2.0.4",
    "connected-react-router": "^4.5.0",
    "firebase": "^5.5.5",
    "history": "^4.10.1",
    "json-diff": "^0.5.4",
    "lodash": "^4.17.15",
    "moment": ">= 2",
    "moment-range": "^4.0.2",
    "prop-types": "^15.7.2",
    "react": "^16.5.2",
    "react-dates": "^17.2.0",
    "react-dom": "^16.5.2",
    "react-dropzone": "^4.2.10",
    "react-image-fallback": "^7.0.1",
    "react-localize-redux": "2.17.5",
    "react-moment": "^0.8.2",
    "react-redux": "^4.4.8 || ^5.0.7",
    "react-redux-firebase": "^2.3.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0",
    "translate": "^1.1.0",
    "typescript": "3.6.4"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@firebase/app-types": "^0.4.0",
    "@firebase/util": "^0.2.21",
    "@types/classnames": "^2.2.9",
    "@types/clipboard": "^2.0.1",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/enzyme-to-json": "^1.5.3",
    "@types/history": "^4.7.3",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.144",
    "@types/moment": "^2.13.0",
    "@types/moment-range": "^3.1.2",
    "@types/react": "^16.9.5",
    "@types/react-dates": "^17.1.5",
    "@types/react-dom": "^16.9.1",
    "@types/react-dropzone": "^5.1.0",
    "@types/react-redux": "^7.1.4",
    "@types/react-router-dom": "^5.1.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.2",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "firebase-bolt": "^0.8.4",
    "firebase-tools": "^7.5.0",
    "jest": "^24.9.0",
    "node-sass": "^4.12.0",
    "p-locate": "^2.0.0",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.18.2",
    "sass": "^1.23.0",
    "ts-jest": "^24.1.0"
  }
}
